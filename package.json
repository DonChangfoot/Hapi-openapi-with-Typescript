{
  "name": "test-hapi-openapi",
  "version": "0.1.0",
  "description": "Testing hapi-openapi with typescript",
  "main": "dist/index.js",
  "license": "Apache-2.0",
  "keywords": [],
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
